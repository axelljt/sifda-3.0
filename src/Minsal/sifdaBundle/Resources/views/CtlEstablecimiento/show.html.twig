{#{% extends '::base.html.twig' %}#}
{#{% extends 'MinsalsifdaBundle::layout.html.twig' %}#}
{% extends 'MinsalsifdaBundle::layoutResponsable.html.twig' %}

{% block contenido_in %}
    
    <h1>Informacion Establecimiento</h1>

    <table id="tabla" class="table table-bordered table-hover table-condensed">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ entity.nombre }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('sifda_ctlestablecimiento') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('sifda_ctlestablecimiento_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
