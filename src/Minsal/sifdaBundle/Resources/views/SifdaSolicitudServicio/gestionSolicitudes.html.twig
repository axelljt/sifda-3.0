            {
                id:valor
            }).done(function(){
                {#alert("Solicitud Rechazada Exitosamente");#}
                 window.location='{{path('sifda_gestionSolicitudes')}}';   
                ('#dialog').attr('title','Confirmacion').text('Solicitud Rechazada Correctamente').dialog();               
                
            });
    }
    
