{#{% extends '::base.html.twig' %}#}
{% extends 'MinsalsifdaBundle::layoutResponsable.html.twig' %}

{% block contenido_in %}
    <h1>CtlDependenciaEstablecimiento list</h1>

    <table id="tabla" class="table table-bordered table-hover table-condensed">
        <thead>
            <tr>
                <th>Id</th>
                <th>Abreviatura</th>
                <th>Habilitado</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('ctldependenciaestablecimiento_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.abreviatura }}</td>
                <td>{{ entity.habilitado }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('ctldependenciaestablecimiento_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('ctldependenciaestablecimiento_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('ctldependenciaestablecimiento_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
